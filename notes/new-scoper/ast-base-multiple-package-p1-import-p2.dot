digraph G {
        subgraph legend {
            rankdir = LR
            node [shape = plaintext]
            label = "Legend";
            key [label = <<table border="0" cellpadding="2" cellspacing="0" cellborder="0">
                <tr><td align="right" port="i1">import</td></tr>
                <tr><td align="right" port="i2">child</td></tr>
                <tr><td align="right" port="i3">reference</td></tr>
                <tr><td align="right" port="i4">item 4</td></tr>
                </table>>] 
            key2 [label = <<table border="0" cellpadding="2" cellspacing="0" cellborder="0">
                <tr><td port="i1">&nbsp;</td></tr>
                <tr><td port="i2">&nbsp;</td></tr>
                <tr><td port="i3">&nbsp;</td></tr>
                <tr><td port="i4">&nbsp;</td></tr>
            </table>>]
            key : i1 : e -> key2 : i1 : w [style = dashed, color=magenta]
            key : i2 : e -> key2 : i2: w [color = black]
            key : i3: e -> key2 : i3 : w [style=dashed]
            key : i4 : e -> key2: i4 : w
            
            {rank = same; key; key2 }
        }
    subgraph AST {
    Model [peripheries=2] 
    P1 [peripheries=2]
    P2 [peripheries=2]

    P1 -> P2 [color=magenta, style=dashed, label="imports", fontcolor=magenta]

    IntfaceD [peripheries=2]
    P2 -> IntfaceD

    Model -> P1
    Model -> P2
    {rank=same; P1; P2}
    
    P1 -> ClassA
    ClassA -> IntfaceD [color=magenta, style=dashed, label="implements", fontcolor=magenta]
    {rank=same; ClassA, IntfaceD}
    }
    {rank=same; legend, AST}
}
