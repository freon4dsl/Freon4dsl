editor default

UnitA {[
Unit A named ${self.name}
    reference showing what is visible here: ${self.myRef}
    unnamed children: ${self.childrenNoName}
    named children: ${self.childrenWithName}
]}

UnitB {[
Unit B named ${self.name}
    reference showing what is visible here: ${self.myRef}
    unnamed children: ${self.childrenNoName}
    named children: ${self.childrenWithName}
]}

AstNode {[
Ast node without name
    reference showing what is visible here: ${self.myRef}
    unnamed children: ${self.childrenNoName}
    named children: ${self.childrenWithName}
]}

AstNodeWithName {[
Ast node named ${self.name}
    reference showing what is visible here: ${self.myRef}
    unnamed children: ${self.childrenNoName}
    named children: ${self.childrenWithName}
]}
