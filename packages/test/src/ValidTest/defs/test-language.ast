language test1 // without 'implements'

model ROOT {
    aa: AAAAAA[];
}

modelunit AAAAAA  {
    name: identifier;
    // properties with concept as type
    AAprop7: BB;
    AAprop7a: CC;
    AAprop7b: SubCC;
}

concept BB base CC {
}

concept CC {
    CCprop3: number;
}

concept SubCC base CC implements DD {
}

interface DD base EE {
    DDprop3: number;
}

interface EE base FF {
}

interface FF {
    FFprop3: number;
}

/* model unit used to test the content of error messages */
modelunit BBBBBB  {
    name: identifier;
    // properties with concept as type
    BBprop1: ZZ;
    BBprop2: YY;
    BBprop3: XX;
}

concept ZZ {
    name: string;
    ZZprop1: YY;
    ZZlist: XX[];
}

concept YY {
    name: string;
    YYprop1: XX;
}

concept XX {
    name: string;
    XXprop1: boolean;
}
