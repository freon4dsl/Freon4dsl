{
  "private": true,
  "name": "@freon4dsl/test",
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/freon4dsl/Freon4dsl"
  },
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules/ && rm -rf dist/",
    "cleanup": "bash ../../scripts/cleanup.sh",
    "demo": "bash ../../scripts/freon-test-dev.sh all -d src/demo/defs -o src/demo/freon",
    "octopus": "bash ../../scripts/freon-test-dev.sh all -d src/octopus-small/defs -o src/octopus-small/freon",
    "parser-conc": "bash ../../scripts/freon-test-dev.sh all -d src/parser-basic-concepts/defs -o src/parser-basic-concepts/freon",
    "basic-props": "bash ../../scripts/freon-test-dev.sh all -d src/parser-basic-properties/defs -o src/parser-basic-properties/freon",
    "parsergen": "bash ../../scripts/freon-test-dev.sh all -d src/parser_gen/defs -o src/parser_gen/freon",
    "parser-extra": "bash ../../scripts/freon-test-dev.sh all -d src/parser-extra-tests/defs -o src/parser-extra-tests/freon",
    "imports": "bash ../../scripts/freon-test-dev.sh all -d src/testCircularImports/def -o src/testCircularImports/freon",
    "defaultscoper": "bash ../../scripts/freon-test-dev.sh all -d src/testDefaultScoper/defs -o src/testDefaultScoper/freon",
    "definedscoper": "cp src/testDefaultScoper/defs/Language.ast src/testDefinedScoper/defs/Language.ast && cp src/testDefaultScoper/__tests__/SimpleModelCreator.ts src/testDefinedScoper/__tests__/ModelCreator.ts && bash ../../scripts/freon-test-dev.sh all -d src/testDefinedScoper/defs -o src/testDefinedScoper/freon",
    "qualified-name": "bash ../../scripts/freon-test-dev.sh all -d src/qualifiedNameScoper/defs -o src/qualifiedNameScoper/freon",
    "scoper-test": "bash ../../scripts/freon-test-dev.sh all -d src/scoperTest/defs -o src/scoperTest/freon",
    "langconstr": "bash ../../scripts/freon-test-dev.sh all -d src/testLangConstructs/defs -o src/testLangConstructs/freon",
    "noparser": "bash ../../scripts/freon-test-dev.sh all -d src/testNoParserAvailable/defs -o src/testNoParserAvailable/freon",
    "prim-gen": "bash ../../scripts/freon-test-dev.sh all -d src/testPrimPropsGeneration/defs -o src/testPrimPropsGeneration/freon",
    "storage": "bash ../../scripts/freon-test-dev.sh all -d src/storage/defs -o src/storage/freon",
    "testproject": "bash ../../scripts/freon-test-dev.sh all -d src/testproject/defs -o src/testproject/freon",
    "typer-test8": "bash ../../scripts/freon-test-dev.sh all -d src/typer-test8/defs -o src/typer-test8/freon",
    "undo-tester": "bash ../../scripts/freon-test-dev.sh all -d src/UndoTester/defs -o src/UndoTester/freon",
    "valid-test": "bash ../../scripts/freon-test-dev.sh all -d src/ValidTest/defs -o src/ValidTest/freon",
    "vehicles": "bash ../../scripts/freon-test-dev.sh all -d src/vehicles/defs -o src/vehicles/freon",
    "build": "npm run demo && npm run storage && npm run octopus && npm run parser-conc && npm run basic-props && npm run parsergen && npm run parser-extra && npm run imports && npm run defaultscoper && npm run definedscoper && npm run langconstr && npm run noparser && npm run prim-gen && npm run testproject && npm run typer-test8 && npm run undo-tester && npm run vehicles && npm run qualified-name && npm run valid-test && npm run scoper-test && tsc",
    "clean-config": "rm */*/config/FreonConfiguration.ts",
    "clean-custom": "rm -r src/*/custom",
    "clean-parser": "rm -r */*/*parse*",
    "clean-code": "npm run clean-config && npm run clean-custom",
    "clean-demo": "bash ../../scripts/freon-test-dev.sh clean-it -d src/demo/defs -o src/demo",
    "clean-octopus": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/octopus-small/defs -o src/octopus-small",
    "clean-parser-conc": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/parser-basic-concepts/defs -o src/parser-basic-concepts",
    "clean-basic-props": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/parser-basic-properties/defs -o src/parser-basic-properties",
    "clean-parsergen": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/parser_gen/defs -o src/parser_gen",
    "clean-imports": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/testCircularImports/def -o src/testCircularImports",
    "clean-defaultscoper": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/testDefaultScoper/defs -o src/testDefaultScoper",
    "clean-definedscoper": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/testDefinedScoper/defs -o src/testDefinedScoper",
    "clean-langconstr": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/testLangConstructs/defs -o src/testLangConstructs",
    "clean-noparser": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/testNoParserAvailable/defs -o src/testNoParserAvailable",
    "clean-prim-gen": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/testPrimPropsGeneration/defs -o src/testPrimPropsGeneration",
    "clean-testproject": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/testproject/defs -o src/testproject",
    "clean-typer-test8": "bash ../../scripts/freon-test-dev.sh clean-it -d src/typer-test8/defs -o src/typer-test8",
    "clean-undo-tester": "bash ../../scripts/freon-test-dev.sh clean-it -d src/UndoTester/defs -o src/UndoTester",
    "clean-valid-test": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/ValidTest/defs -o src/ValidTest",
    "clean-vehicles": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/vehicles/defs -o src/vehicles",
    "clean-qualified-name": "bash ../../scripts/freon-test-dev.sh clean-it -f -d src/qualifiedNameScoper/defs -o src/qualifiedNameScoper",
    "clean-all-projects": "npm run clean-demo && npm run clean-octopus && npm run clean-parser-conc && npm run clean-basic-props && npm run clean-parsergen && npm run clean-imports && npm run clean-defaultscoper && npm run clean-definedscoper && npm run clean-langconstr && npm run clean-noparser && npm run clean-prim-gen && npm run clean-testproject && npm run clean-typer-test8 && npm run clean-vehicles && npm run clean-qualified-name && npm run clean-valid-test",
    "test": "vitest run",
    "check-imports": "bash ../../scripts/check-import-extensions.sh"
  },
  "devDependencies": {
    "vitest": "3.2.4",
    "@vitest/ui": "3.2.4"
  },
  "dependencies": {
    "@freon4dsl/core": "~2.0.0",
    "@freon4dsl/meta": "~2.0.0",
    "@freon4dsl/test-helpers": "~2.0.0",
    "@lionweb/validation": "^0.6.3",
    "protobufjs": "7.5.4",
    "global": "^4.4.0",
    "kotlin": "^1.5.21",
    "mobx": "^6.12.3",
    "net.akehurst.language-agl-processor": "4.2.1-20-RC2",
    "tslib": "^2.8.1"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
