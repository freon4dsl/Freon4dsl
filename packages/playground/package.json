{
  "name": "@projectit/playground",
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "echo No build for playground",
    "build-app": "rollup -c",
    "dev": "cross-env NODE_PORT=3002 rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check --tsconfig ./tsconfig.json",
    "errors-only": "svelte-check --threshold error --tsconfig ./tsconfig.json",
    "install-local": "yarn install --registry http://localhost:4873",
    "prepare-app": "npm run styles-light && npm run styles-dark",
    "styles-light": "smui-theme compile public/site.css -i src/webapp/style",
    "styles-dark": "smui-theme compile public/site-dark.css -i src/webapp/style/dark",
    "graphviz":	    "bash ../../scripts/projectit-dev.sh -v all -d src/graphviz/defs -o src/graphviz",
    "docu-project": "bash ../../scripts/projectit-dev.sh -v all -d src/DocuProject/defs -o src/DocuProject",
    "example": 	    "bash ../../scripts/projectit-dev.sh -v all -d src/example/defs -o src/example",
    "references":   "bash ../../scripts/projectit-dev.sh -v all -d src/complex-reference-generated/defs -o src/complex-reference-generated",
    "rules":        "bash ../../scripts/projectit-dev.sh -v all -d src/RulesLanguage/defs -o src/RulesLanguage",
    "watch-example":"bash ../../scripts/projectit-dev.sh -w -v all -d src/example/defs -o src/example",
    "ex-parser":    "bash ../../scripts/projectit-dev.sh -v parse-it -d src/example/defs -o src/example",
    "calculator":   "bash ../../scripts/projectit-dev.sh all -d src/calculator/defs -o src/calculator",
    "exptest":  "bash ../../scripts/projectit-dev.sh all -d src/ExpTest/defs -o src/ExpTest",
    "mps-expressions":  "bash ../../scripts/projectit-dev.sh all -d src/MpsExpressions/defs -o src/MpsExpressions",
    "heating":      "bash ../../scripts/projectit-dev.sh all -d src/heat/defs -o src/heat",
    "json": 	    "bash ../../scripts/projectit-dev.sh all -d src/Json/defs -o src/Json",
    "expression-library":    "bash ../../scripts/projectit-dev.sh -v all -d src/Expresionlibrary/defs -o src/Expresionlibrary",
    "metatyper":    "bash ../../scripts/projectit-dev.sh -v all -d src/metatyper/defs -o src/metatyper",
    "octopus": 	    "bash ../../scripts/projectit-dev.sh all -d src/octopus/defs -o src/octopus",
    "octopus-editor": 	    "bash ../../scripts/projectit-dev.sh -w edit-it -d src/octopus/defs -o src/octopus",
    "openhab":      "bash ../../scripts/projectit-dev.sh -v all -d src/openhab/defs -o src/openhab",
    "pilanguage": "bash ../../scripts/projectit-dev.sh -v all -d src/PiLanguage/defs -o src/PiLanguage",
    "undo": "bash ../../scripts/projectit-dev.sh -v all -d src/UndoTester/defs -o src/UndoTester",
    "simple": "bash ../../scripts/projectit-dev.sh -v all -d src/SimpleProject/defs -o src/SimpleProject",
    "taxrules": 	"bash ../../scripts/projectit-dev.sh all -d src/TaxRrules/defs -o src/TaxRules",
    "clean-graphviz":	  "bash ../../scripts/projectit-dev.sh clean-it -f -d src/graphviz/defs -o src/graphviz",
    "clean-example": 	  "bash ../../scripts/projectit-dev.sh clean-it -d src/example/defs -o src/example",
    "clean-calculator":   "bash ../../scripts/projectit-dev.sh clean-it -f -d src/calculator/defs -o src/calculator",
    "clean-expressions":  "bash ../../scripts/projectit-dev.sh clean-it -f -d src/MpsExpressions/defs -o src/MpsExpressions",
    "clean-heating":      "bash ../../scripts/projectit-dev.sh clean-it -f -d src/heating/defs -o src/heating",
    "clean-json": 	      "bash ../../scripts/projectit-dev.sh clean-it -f -d src/Json/defs -o src/Json",
    "clean-octopus": 	  "bash ../../scripts/projectit-dev.sh clean-it -f -d src/octopus/defs -o src/octopus",
    "clean-openhab":      "bash ../../scripts/projectit-dev.sh clean-it -f -d src/openhab/defs -o src/openhab",
    "clean-pi-languages": "bash ../../scripts/projectit-dev.sh clean-it -f -d src/PiLanguage/defs -o src/PiLanguage",
    "clean-taxrules": 	  "bash ../../scripts/projectit-dev.sh clean-it -f -d src/TaxRules/defs -o src/TaxRules",
    "clean-rules": 	      "bash ../../scripts/projectit-dev.sh clean-it -f -d src/RulesLanguage/defs -o src/RulesLanguage",
    "save-language":        "ts-node ../tools/src/InstallLanguage.ts save",
    "install-example":      "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/Example -t ./src",
    "install-docu-project": "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/DocuProject -t ./src",
    "install-rules":        "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/RulesLanguage -t ./src",
    "install-calculator":   "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/calculator -t ./src",
    "install-json":         "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/Json -t ./src",
    "install-expressionlib": "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/ExpressionLibrary -t ./src",
    "install-octopus":      "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/octopus -t ./src",
    "install-openhab":      "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/openhab -t ./src",
    "install-graphviz":     "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/graphviz -t ./src",
    "install-expressions":  "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/MpsExpressions -t ./src",
    "install-heating":      "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/heat -t ./src",
    "install-exp-test":     "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/ExpTest -t ./src",
    "install-tax-rules":    "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/TaxRules -t ./src",
    "install-pi-language":  "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/PiLanguage -t ./src",
    "install-undo":         "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/UndoTester -t ./src",
    "install-simple":       "ts-node ../tools/src/InstallLanguage.ts install -d ../samples/SimpleProject -t ./src",
    "clean-libraries":    "bash ../../scripts/projectit-dev.sh clean-it -f -d src/libraries-test/defs -o src/libraries-test",
    "clean-all-examples": "yarn clean-calculator && yarn clean-example && yarn clean-graphviz && yarn clean-heating && yarn clean-json && yarn clean-libraries && yarn clean-expressions && yarn clean-octopus && yarn clean-openhab && yarn clean-pi-languages && yarn clean-rules && yarn clean-taxrules",
    "clean-config": "rm */*/projectit/ProjectItConfiguration.ts",
    "clean-custom": "rm */*/*/Custom*.ts",
    "clean":        "yarn clean-config && yarn clean-custom"
  },
  "devDependencies": {
    "@material/theme": "^13.0.0",
    "@material/typography": "^13.0.0",
    "@mdi/js": "^6.6.96",
    "@smui/banner": "^6.0.0-beta.16",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/card": "^6.0.0-beta.16",
    "@smui/checkbox": "^6.0.0-beta.16",
    "@smui/common": "^6.0.0-beta.16",
    "@smui/data-table": "^6.0.0-beta.16",
    "@smui/dialog": "^6.0.0-beta.16",
    "@smui/drawer": "^6.0.0-beta.16",
    "@smui-extra/autocomplete": "^6.0.0-beta.16",
    "@smui/form-field": "^6.0.0-beta.16",
    "@smui/icon-button": "^6.0.0-beta.16",
    "@smui/layout-grid": "^6.0.0-beta.16",
    "@smui/linear-progress": "^6.0.0-beta.16",
    "@smui/list": "^6.0.0-beta.16",
    "@smui/menu": "^6.0.0-beta.16",
    "@smui/menu-surface": "^6.0.0-beta.16",
    "@smui/radio": "^6.0.0-beta.16",
    "@smui/paper": "^6.0.0-beta.16",
    "@smui/tab": "^6.0.0-beta.16",
    "@smui/tab-bar": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "smui-theme": "^6.0.0-beta.16",
    "@smui/tooltip": "^6.0.0-beta.16",
    "@smui/top-app-bar": "^6.0.0-beta.16",
    "@rollup/plugin-commonjs": "^17.0.0",
    "rollup-plugin-inject-process-env": "1.3.1",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@tsconfig/svelte": "^3.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@projectit/core": "0.5.0",
    "@projectit/core-svelte": "0.5.0",
    "@projectit/server": "0.5.0",
    "@types/node": "^14.14.31",
    "mobx": "6.3.3",
    "mobx-utils": "6.0.4",
    "click-outside": "1.0.4",
    "lodash": "^4.17.21",
    "net.akehurst.language-agl-processor": "4.0.0-rc.5",
    "kotlin": "^1.5.21",
    "sirv-cli": "^1.0.0",
    "tslib": "^2.4.0"
  }
}
