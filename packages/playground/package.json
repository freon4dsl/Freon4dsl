{
  "name": "@projectit/playground",
  "version": "0.2.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn example && tsc",
    "build-app": "rollup -c",
    "dev": "cross-env NODE_PORT=3002 rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "errors-only": "svelte-check --threshold error",
    "install-local": "yarn install --registry http://localhost:4873",
    "clean": "rm src/example/*/*/*.js && rm src/example/*/*/*.js.map",
    "graphviz":	    "bash ../../scripts/projectit-dev.sh -v all -d src/graphviz/defs -o src/graphviz",
    "example": 	    "bash ../../scripts/projectit-dev.sh -v all -d src/example/defs -o src/example",
    "rules":        "bash ../../scripts/projectit-dev.sh -v all -d src/rules-language/defs -o src/rules-language",
    "watch-example":"bash ../../scripts/projectit-dev.sh -w -v all -d src/example/defs -o src/example",
    "ex-parser":    "bash ../../scripts/projectit-dev.sh -v parse-it -d src/example/defs -o src/example",
    "calculator":   "bash ../../scripts/projectit-dev.sh all -d src/calculator/defs -o src/calculator",
    "expressions":  "bash ../../scripts/projectit-dev.sh all -d src/mps-expressions/defs -o src/mps-expressions",
    "heating":      "bash ../../scripts/projectit-dev.sh all -d src/heating/defs -o src/heating",
    "json": 	    "bash ../../scripts/projectit-dev.sh all -d src/json/defs -o src/json",
    "libraries":    "bash ../../scripts/projectit-dev.sh -v all -d src/libraries-test/defs -o src/libraries-test",
    "octopus": 	    "bash ../../scripts/projectit-dev.sh all -d src/octopus/defs -o src/octopus",
    "octopus-editor": 	    "bash ../../scripts/projectit-dev.sh -w edit-it -d src/octopus/defs -o src/octopus",
    "openhab":      "bash ../../scripts/projectit-dev.sh -v all -d src/openhab/defs -o src/openhab",
    "pi-languages": "bash ../../scripts/projectit-dev.sh -v all -d src/pi-languages/defs -o src/pi-languages",
    "taxrules": 	"bash ../../scripts/projectit-dev.sh all -d src/taxrules/defs -o src/taxrules",
    "all-examples": "yarn calculator && yarn octopus && yarn example && yarn heating && yarn json && yarn libraries && yarn openhab && yarn pi-languages && yarn taxrules && yarn expressions && yarn rules && yarn openhab",
    "clean-graphviz":	  "bash ../../scripts/projectit-dev.sh clean-it -f -d src/graphviz/defs -o src/graphviz",
    "clean-example": 	  "bash ../../scripts/projectit-dev.sh clean-it -d src/example/defs -o src/example",
    "clean-calculator":   "bash ../../scripts/projectit-dev.sh clean-it -f -d src/calculator/defs -o src/calculator",
    "clean-expressions":  "bash ../../scripts/projectit-dev.sh clean-it -f -d src/mps-expressions/defs -o src/mps-expressions",
    "clean-heating":      "bash ../../scripts/projectit-dev.sh clean-it -f -d src/heating/defs -o src/heating",
    "clean-json": 	      "bash ../../scripts/projectit-dev.sh clean-it -f -d src/json/defs -o src/json",
    "clean-octopus": 	  "bash ../../scripts/projectit-dev.sh clean-it -f -d src/octopus/defs -o src/octopus",
    "clean-openhab":      "bash ../../scripts/projectit-dev.sh clean-it -f -d src/openhab/defs -o src/openhab",
    "clean-pi-languages": "bash ../../scripts/projectit-dev.sh clean-it -f -d src/pi-languages/defs -o src/pi-languages",
    "clean-taxrules": 	  "bash ../../scripts/projectit-dev.sh clean-it -f -d src/taxrules/defs -o src/taxrules",
    "clean-all-examples": "yarn clean-calculator && yarn clean-octopus && yarn clean-example && yarn clean-heating && yarn clean-json && yarn clean-expressions && yarn clean-openhab && yarn clean-pi-languages && yarn clean-taxrules && yarn clean-expressions"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "rollup-plugin-inject-process-env": "1.3.1",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@tsconfig/svelte": "^3.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@projectit/core": "0.2.0",
    "@projectit/core-svelte": "0.2.0",
    "@projectit/server": "0.2.0",
    "@types/node": "^14.14.31",
    "csstype": "^3.0.10",
    "mobx": "6.3.3",
    "mobx-utils": "6.0.4",
    "click-outside": "1.0.4",
    "lodash": "^4.17.21",
    "net.akehurst.language-agl-processor": "4.0.0-rc.5",
    "kotlin": "^1.5.21",
    "sirv-cli": "^1.0.0",
    "svelte-mui": "0.4.0-2",
    "tslib": "^2.3.0"
  }
}
