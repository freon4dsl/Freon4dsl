editor myTables

Parameter {
    table [
        Type            | Name
        ${declaredType} | ${name}
    ]
}
Attribute {
    table [
        ${name} | ${declaredType}
    ]
}
Method {
    table [
        Name    | parameters    | type            | body
        ${name} | ${parameters} | ${declaredType} | ${body}
    ]
}
