{
  "publishConfig": {
    "access": "public"
  },
  "name": "@freon4dsl/weblib-smui",
  "version": "1.1.0-beta.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freon4dsl/Freon4dsl.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "svelte": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf node_modules/",
    "dev": "vite dev",
    "build": "vite build && npm run prepack && publint",
    "build-dev": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package",
    "publint": "publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "build-release": "npm run build",
    "validate": "svelte-check",
    "errors-only": "svelte-check --threshold error --tsconfig ./tsconfig.json",
    "install-local": "npm install --registry http://localhost:4873",
    "republish-local": "npm run unpublish-local && npm run publish-local",
    "release": "npm publish",
    "publish-local": "npm publish --registry http://localhost:4873",
    "unpublish-local": "npm unpublish --force --registry http://localhost:4873"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "devDependencies": {
    "@material/theme": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@mdi/js": "^7.4.47",
    "@smui/banner": "^8.0.0-beta.3",
    "@smui/button": "^8.0.0-beta.3",
    "@smui/card": "^8.0.0-beta.3",
    "@smui/checkbox": "^8.0.0-beta.3",
    "@smui/common": "^8.0.0-beta.3",
    "@smui/data-table": "^8.0.0-beta.3",
    "@smui/dialog": "^8.0.0-beta.3",
    "@smui/drawer": "^8.0.0-beta.3",
    "@smui/form-field": "^8.0.0-beta.3",
    "@smui/icon-button": "^8.0.0-beta.3",
    "@smui/layout-grid": "^8.0.0-beta.3",
    "@smui/linear-progress": "^8.0.0-beta.3",
    "@smui/list": "^8.0.0-beta.3",
    "@smui/menu": "^8.0.0-beta.3",
    "@smui/menu-surface": "^8.0.0-beta.3",
    "@smui/paper": "^8.0.0-beta.3",
    "@smui/radio": "^8.0.0-beta.3",
    "@smui/tab": "^8.0.0-beta.3",
    "@smui/tab-bar": "^8.0.0-beta.3",
    "@smui/textfield": "^8.0.0-beta.3",
    "@smui/tooltip": "^8.0.0-beta.3",
    "@smui/top-app-bar": "^8.0.0-beta.3",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "click-outside": "^2.0.2",
    "mobx": "^6.12.13",
    "publint": "^0.3.5",
    "sass": "^1.77.6",
    "smui-theme": "^8.0.0-beta.3",
    "svelte-check": "^4.1.4",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "@freon4dsl/core": "1.1.0-beta.3",
    "@freon4dsl/core-svelte": "1.1.0-beta.3",
    "@material/theme": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@mdi/js": "^7.4.47",
    "click-outside": "^2.0.2",
    "mobx": "^6.12.13",
    "svelte": "5.25.3"
  }
}
