{
  "name": "@freon4dsl/samples-education",

  "version": "2.0.0-beta.5",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules/",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "generateTypes": "tsc -p tsconfig.build.json",
    "build": "npm run generate && npm run typecheck && npm run generateTypes && rollup -c",
    "build-samples": "npm run build",
    "compile": "tsc && rollup --config",
    "install-local": "npm install --registry http://localhost:4873",
    "generate": "bash ../../../scripts/freon-samples-dev.sh -v all -d src/defs -o src/",
    "clean-gen": "bash ../../../scripts/freon-samples-dev.sh clean-it -d src/defs -o src/",
    "clean-config": "rm */config/FreonConfiguration.ts",
    "clean-custom": "rm */*/Custom*.ts",
    "clean-all": "npm run clean-gen && npm run clean-config && npm run clean-custom"
  },
  "devDependencies": {
    "@freon4dsl/meta": "2.0.0-beta.5"
  },
  "dependencies": {
    "@freon4dsl/core": "2.0.0-beta.5",
    "@types/node": "^20.14.1",
    "kotlin": "^1.5.21",
    "mobx": "^6.12.3",
    "mobx-utils": "^6.0.8",
    "net.akehurst.language-agl-processor": "4.2.1-20-RC2",
    "tslib": "^2.6.2"
  }
}
