language Qname

model QnameModel {
    units: StarterUnit[];
}

concept Variable {
    name: identifier;
}

modelunit StarterUnit {
    modules: Module[];
}

concept Module {
    name: identifier;
    varDefinitions: Variable[];
    varReferences: VarReference[];
}

abstract concept VarReference {
}

concept ComplexVarReference base VarReference {
    target: VarReference;
    reference module: Module;
}

concept SimpleVarReference base VarReference {
    reference target: Variable;
}
