/* This file contains the default editor definition. */

editor default

Flow {[
    Flow ${self.name} for subject ${self.subject}

    ${self.rules vertical}
]}

FlowRule {[
    -------------------------------------
    Name: ${self.name}
        Description: ${self.description}
        For page ${self.page}

        ${self.transitions}
]}

PageTransition {[
    when ${self.condition} goto page ${self.toPage}
]}
