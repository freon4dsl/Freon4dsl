{
  "name": "@freon4dsl/samples-fre-language",
  "version": "2.1.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules/",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "generateTypes": "tsc -p tsconfig.build.json",
    "build": "npm run generate && npm run typecheck && npm run generateTypes && rollup -c",
    "build-samples": "npm run build",
    "compile": "tsc && rollup --config",
    "install-local": "npm install --registry http://localhost:4873",
    "generate": "bash ../../../scripts/freon-samples-dev.sh -v all",
    "check-imports": "bash ../../../scripts/check-import-extensions.sh",
    "generate-ast": "bash ../../../scripts/freon-samples-dev.sh -v ast-it",
    "generate-edit": "bash ../../../scripts/freon-samples-dev.sh -v edit-it",
    "generate-interpret": "bash ../../../scripts/freon-samples-dev.sh -v interpret-it",
    "generate-parse": "bash ../../../scripts/freon-samples-dev.sh -v parse-it",
    "generate-scope": "bash ../../../scripts/freon-samples-dev.sh -v scope-it",
    "generate-valid": "bash ../../../scripts/freon-samples-dev.sh -v validate-it",
    "generate-type": "bash ../../../scripts/freon-samples-dev.sh -v type-it",
    "generate-diagram": "bash ../../../scripts/freon-samples-dev.sh -v diagram-it",
    "generate-lionweb": "bash ../../../scripts/freon-samples-dev.sh -v lionweb-it"
  },
  "devDependencies": {
    "@freon4dsl/meta": "~2.1.0",
    "@freon4dsl/test-helpers": "~2.1.0"
  },
  "dependencies": {
    "@freon4dsl/core": "~2.1.0",
    "@types/node": "^20.14.1",
    "kotlin": "^1.5.21",
    "mobx": "^6.12.3",
    "mobx-utils": "^6.0.8",
    "net.akehurst.language-agl-processor": "4.2.1-20-RC2",
    "tslib": "^2.6.2"
  }
}
