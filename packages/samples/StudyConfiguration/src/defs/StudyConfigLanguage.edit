editor default

//boolean [YES | NO] // are the strings used to display a boolean value

Period {
    [
        Period: ${name}
        Description: ${description}
        Date: ${date}
        EVENTS
            ${events}
    ]
}

Event {
    [
            Event: ${name}
            Description: ${description}
            Schedule: 
                ${schedule}

            Checklist of Tasks:    ${showSequenced} Allow Sequencing
                ${checkList }

    ]  trigger = "Event"
}

CheckList {
    [
    ${activities table rows}
    ]
}

Task {
    table [
             Tasks    
             ${name}

        ]

}


Step {
    [
            Step ${title} 
            Description:
            ${detailsDescription}

            References:
                ${references}

            Systems Used:
                ${systems}

            People:
                ${people}

    ]
}


SystemAccess {
    [ 

        System Name  : ${name}  Function: ${functionName}
        Description  : ${description}
        Access at ${accessedAt}
        The Robot should copy:
            ${robotMappings}



    ]
}

SystemAccessReference {
    [
        System: ${system}
    ]
}

AccessedAtURL {
    [
        URL: ${url}
    ]
}

AccessedAPhoneNumber {
    [
        Phone Number: ${phoneNumber}
    ]
}

Person {
    [
        Name: ${name}  ${useSystemContact} Use System Contact   Email: ${email}
                                                                Phone: ${phoneNumber}
                                                                Role : ${role} 
    ]
}

Staffing {
    [

        Roles: 
            ${roles}
        Assignments: 
            ${assignments}
    ]
}

Reference {
    [
        Reference Title: ${title}    Link: ${link}
        Description    : ${description}

    ]
}

Assignment {
    [
        Role  : ${role}
        Person: ${person}
    ]
}

StaffRole {
    [
        role: ${name}
    ]
}

WorkflowDescription {
    [
    Description: ${text}
    ]
    trigger = "Description"
}

WorkflowBranch {
    [ Branch: ${branch}  ]
}

RobotMapping {
    [
    From this Page: ${sourcePage}
    To this Page :   ${targetPage}
    These fields:
        ${sourceToTargetMappings}
    ]
}

SourceToTargetMapping {
    [
        Source Field: ${sourceField}  Target Field: ${targetField}
    ]
}

Description {
   [ ${text} ]
}

