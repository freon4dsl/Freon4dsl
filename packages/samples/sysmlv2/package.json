{
  "name": "@freon4dsl/samples-sysmlv2",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules/",
    "build": "echo 'No build for sysmlv2'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "generateTypes": "tsc -p tsconfig.build.json",
    "build2": "npm run generate && npm run typecheck && npm run generateTypes && rollup -c",
    "compile": "tsc && rollup --config",
    "install-local": "npm install --registry http://localhost:4873",
    "profile": "bash ../../../scripts/freon-profiling-dev.sh -v all -d src/defs -o src/",
    "generate": "bash ../../../scripts/freon-samples-dev.sh -v all -d src/defs -o src/",
    "generate-ast": "bash ../../../scripts/freon-samples-dev.sh -v ast-it -d src/defs -o src/",
    "generate-edit": "bash ../../../scripts/freon-samples-dev.sh -v edit-it -d src/defs -o src/",
    "generate-parse": "bash ../../../scripts/freon-samples-dev.sh -v parse-it -d src/defs -o src/",
    "generate-scope": "bash ../../../scripts/freon-samples-dev.sh -v scope-it -d src/defs -o src/",
    "generate-type": "bash ../../../scripts/freon-samples-dev.sh -v type-it -d src/defs -o src/",
    "clean-gen": "bash ../../../scripts/freon-samples-dev.sh clean-it -d src/defs -o src/",
    "clean-config": "rm */config/FreonConfiguration.ts",
    "clean-custom": "rm */*/Custom*.ts",
    "clean-all": "npm run clean-gen && npm run clean-config && npm run clean-custom"
  },
  "devDependencies": {
    "@freon4dsl/meta": "~1.0.2"
  },
  "dependencies": {
    "@freon4dsl/core": "~1.0.2",
    "@types/node": "^20.14.1",
    "kotlin": "^1.5.21",
    "mobx": "^6.12.3",
    "mobx-utils": "^6.0.8",
    "net.akehurst.language-agl-processor": "4.2.1-20-RC2",
    "tslib": "^2.6.2"
  }
}
