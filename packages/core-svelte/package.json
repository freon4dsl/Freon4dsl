{
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "name": "@projectit/core-svelte",
  "version": "0.4.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joswarmer/ProjectIt.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build-release": "yarn run build",
    "dev": "rollup -c -w",
    "validate": "svelte-check",
    "errors-only": "svelte-check --threshold error --tsconfig ./tsconfig.json",
    "install-local": "yarn install --registry http://localhost:4873",
    "republish-local": "yarn unpublish-local && yarn publish-local",
    "release": "npm publish",
    "publish-local": "npm publish --registry http://localhost:4873",
    "unpublish-local": "npm unpublish --force --registry http://localhost:4873",
    "test": "jest"
  },
  "devDependencies": {
    "@testing-library/svelte": "3.1.0",
    "@testing-library/user-event": "^13.5.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@types/jest": "^27.4.0",
    "svelte-jester": "^2.3.1",
    "ts-jest": "^27.1.3",
    "jest-transform-stub": "^2.0.0"
  },
  "dependencies": {
    "@projectit/core": "0.4.0",
    "click-outside": "1.0.4",
    "mobx": "6.3.3",
    "tslib": "2.4.0"
  }
}
